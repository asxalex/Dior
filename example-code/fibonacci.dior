;;第一种递归求fibonacci的方法
(define fib-1
	(lambda (n)
		(cond ((= n 0) 0)
		      ((= n 1) 1)
		      (else (+ (fib (- n 1))
		      	       (fib (- n 2)))))))

;;第二种, 尾递归
(define fib-iter
	(lambda (a b count)
		(if (= count 0)
		    b
		    (fib-iter (+ a b) a (- count 1)))))
(define fib-2
	(lambda (n)
		(fib-iter 1 0 n)))


(fib-2 100)